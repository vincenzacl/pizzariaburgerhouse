<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webwapsolutions.epizzaappPizzaria Burgerhouse" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pizzaria Burgerhouse</name>
    <description>
        
		Pizzaria Burgerhouse App powered by ePizza Suite Pro - leveret af Web Wap Solutions
    </description>
    <author email="support@webwapsolutions.com" href="http://webwapsolutions.com">
        Web Wap Solutions Team
    </author>
    <dependency id="cordova-plugin-geolocation" />
    <dependency id="cordova-plugin-dialogs" />
    <gap:config-file parent="/manifest/application" platform="android">
        <activity android:launchMode="singleInstance" />
    </gap:config-file>
    <gap:plugin name="org.apache.cordova.battery-status" source="npm" />
    <gap:plugin name="org.apache.cordova.console" source="npm" />
    <gap:plugin name="org.apache.cordova.device" source="npm" />
    <gap:plugin name="org.apache.cordova.device-motion" source="npm" />
    <gap:plugin name="org.apache.cordova.device-orientation" source="npm" />
    <gap:plugin name="org.apache.cordova.dialogs" source="npm" />
    <gap:plugin name="org.apache.cordova.geolocation" source="npm" />
    <gap:plugin name="org.apache.cordova.globalization" source="npm" />
    <gap:plugin name="org.apache.cordova.inappbrowser" source="npm" />
    <gap:plugin name="org.apache.cordova.network-information" source="npm" />
    <gap:plugin name="org.apache.cordova.splashscreen" source="npm" />
    <gap:plugin name="org.apache.cordova.vibration" source="npm" />
    <gap:plugin name="com.phonegap.plugin.statusbar" />
    <access />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/screen/generic.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="res/screen/android/drawable-ldpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="res/screen/android/drawable-mdpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="res/screen/android/drawable-hdpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="res/screen/android/drawable-xhdpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="xxhdpi" src="res/screen/android/drawable-xxhdpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="xxxhdpi" src="res/screen/android/drawable-xxxhdpi/ic_launcher.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="res/screen/android/drawable-ldpi/ic_launcher.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="res/screen/android/drawable-mdpi/ic_launcher.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="res/screen/android/drawable-hdpi/ic_launcher.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="res/screen/android/drawable-xhdpi/ic_launcher.png" />
    <gap:splash gap:platform="ios" height="480" src="res/screen/ios/Default.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="res/screen/ios/Default@2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="res/screen/ios/Default-568h@2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="res/screen/ios/Default-Portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="res/screen/ios/Default-Landscape.png" width="1024" />
    <icon gap:platform="ios" height="180" src="res/screen/ios/Icon-60@3x.png" width="180" />
    <icon gap:platform="ios" height="60" src="res/screen/ios/Icon-60.png" width="60" />
    <icon gap:platform="ios" height="120" src="res/screen/ios/Icon-60@2x.png" width="120" />
    <icon gap:platform="ios" height="76" src="res/screen/ios/Icon-76.png" width="76" />
    <icon gap:platform="ios" height="152" src="res/screen/ios/Icon-76@2x.png" width="152" />
    <icon gap:platform="ios" height="40" src="res/screen/ios/Icon-40.png" width="40" />
    <icon gap:platform="ios" height="80" src="res/screen/ios/Icon-40@2x.png" width="80" />
    <icon gap:platform="ios" height="57" src="res/screen/ios/Icon.png" width="57" />
    <icon gap:platform="ios" height="114" src="res/screen/ios/Icon@2x.png" width="114" />
    <icon gap:platform="ios" height="72" src="res/screen/ios/Icon-72.png" width="72" />
    <icon gap:platform="ios" height="144" src="res/screen/ios/Icon-72@2x.png" width="144" />
    <icon gap:platform="ios" height="29" src="res/screen/ios/Icon-small.png" width="29" />
    <icon gap:platform="ios" height="58" src="res/screen/ios/Icon-small@2x.png" width="58" />
    <icon gap:platform="ios" height="50" src="res/screen/ios/Icon-50.png" width="50" />
    <icon gap:platform="ios" height="100" src="res/screen/ios/Icon-50@2x.png" width="100" />
    <preference name="permissions" value="none" />
    <preference name="SplashScreen" value="generic" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="AndroidLaunchMode" value="singleTask" />
    <preference name="KeepRunning" value="true" />
    <preference name="InAppBrowserStorageEnabled" value="true" />
    <preference name="LoadingDialog" value="Pizzaria Burgerhouse, Velkommen til Pizzaria Burgerhouse" />
    <preference name="LoadingPageDialog" value="Pizzaria Burgerhouse, Velkommen til Pizzaria Burgerhouse" />
    <preference name="LogLevel" value="WARN" />
</widget>
